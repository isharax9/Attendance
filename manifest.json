{
    "manifest_version": 3,
    "name": "Attendance",
    "version": "1.0",
    "description": "Let me mark your attendance..",
    "icons": {
        "16": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    },
    "action": {
        "default_icon": "logo.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "https://web.javainstitute.org/web-portal/main-pages/index-student*"
            ],
            "js": [
                "attendance.js"
            ]
        }
    ],
    "permissions": [
        "tabs"
    ]
}